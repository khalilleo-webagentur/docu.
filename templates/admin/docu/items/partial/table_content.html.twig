<div class="card-body table-responsive bg-white p-0 mb-3">
    <table class="table table-hover text-nowrap">
        <thead>
            <tr class="fs-14">
                <th>#</th>
                <th>{{ 'Slug'|trans }}</th>
                <th>{{ 'Catigory'|trans }}</th>
                <th>{{ 'Likes'|trans }}</th>
                <th>{{ 'disLikes'|trans }}</th>
                <th>{{ 'Updated'|trans }}</th>
                <th>{{ 'Created'|trans }}</th>
                <th>{{ 'Action'|trans }}</th>
            </tr>
        </thead>
        <tbody>
            {% for row in items %}
            <tr class="fs-14">
                <td>{{ row.id }}</td>

                <td>{{ row.slug }}</td>

                <td>
                    <a href="{{path('app_admin_docu_catigory_edit', {id:row.catigory.id})}}">
                        {{ row.catigory.name }}
                    </a>
                </td>

                <td>{{row.likes}}</td>
                <td>{{row.disLikes}}</td>

                <td title="{{ row.updatedAt|date }}">{{ timeAgo(row.updatedAt) }}</td>

                <td title="{{ row.createdAt|date }}">{{ timeAgo(row.createdAt) }}</td>

                <td>
                    <a href="{{path('app_admin_docu_item_edit', {id:row.id})}}">
                        <span class="fa fa-edit fa-sm"></span>
                    </a>
                </td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
</div>